{
	"training_data": "../../datasets/",
  	"output": "../../output/results/",
  	"dataset_division": "721_5fold",

  	"embedding_module": "RDGCN",
  	"alignment_module": "mapping",
	"search_module": "greedy",

    "dim": 300,

	"neg_sampling": "uniform",
	"neg_triple_num": 10,

	"learning_rate": 0.001,
	"max_epoch": 2000,
	"batch_size": 20000,

	"batch_threads_num": 3,
	"test_threads_num": 12,

	"ordered": true,
	"start_valid": 30,
	"eval_freq": 10,
	"stop_metric": "hits1",
	"eval_metric": "manhattan",
	"csls": 10,
	"top_k": [1, 5, 10, 50],
	"is_save": true,
	"eval_norm": false,

    "gamma": 1.0,
    "dropout": 0,
	"beta": 0.3,
    "alpha": 0.1
}