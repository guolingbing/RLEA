{
	"training_data": "../../datasets/",
  	"output": "../../output/results/",
  	"dataset_division": "721_5fold",

  	"embedding_module": "BootEA_RotatE",
  	"alignment_module": "swapping",
	"search_module": "greedy",
	  
    "dim": 100,
  	"init": "normal",
	"ent_l2_norm": true,
	"rel_l2_norm": false,

	"gamma": 12.0,
	  
	"learning_rate": 0.01,
    "optimizer": "Adam",
	"max_epoch": 2000,
	"batch_size": 5000,
	"min_iter": 40,

  	"neg_sampling": "uniform",
	"neg_triple_num": 10,

	"truncated_epsilon": 0.9,
	"truncated_freq": 10,

	"batch_threads_num": 4,
	"test_threads_num": 4,

	"ordered": true,
	"start_valid": 10,
	"start_bp": 5000,
	"eval_freq": 10,
	"stop_metric": "hits1",
	"eval_metric": "inner",
	"csls": 10,
	"top_k": [1, 5, 10, 50],
	"is_save": true,
	"eval_norm": true,

	"sim_th": 0.75,
	"k": 10,

	"sub_epoch": 10,
	"align_times": 1
}